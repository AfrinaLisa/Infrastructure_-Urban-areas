# Create a water distribution network
water_network = nx.Graph()

# Add nodes (junctions) and edges (pipes) with capacities
water_network.add_edges_from([(1, 2, {'capacity': 100}), (2, 3, {'capacity': 50}),
                              (3, 4, {'capacity': 75}), (4, 5, {'capacity': 120})])

# Find the maximum flow between source (1) and sink (5)
flow_value, flow_dict = nx.maximum_flow(water_network, 1, 5)
print(f"Maximum flow from 1 to 5: {flow_value}")
